{
  "name": "gym-backend",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only src/app.ts",
    "dev:prod": "NODE_ENV=production ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "rm -rf dist && tsc",
    "start": "node dist/app.js",
    "deploy:prod": "npx prisma migrate deploy && npm start",
    "migrate": "npx prisma db push",
    "seed": "npx prisma db seed"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.0.0",
    "@types/nodemailer": "^7.0.2",
    "@types/winston": "^2.4.4",
    "prisma": "^5.15.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0"
  },
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "archiver": "^7.0.1",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "fs-extra": "^11.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.9",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  }
}
